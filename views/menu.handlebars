<div class="menu-container">
  <div class="card-header col-md-10">
    <h2>
      &#127869 Our Menu
    </h2>
  </div>
  {{#each menus as |menu|}}
    <div class="dish col-md-5">
      <p>
        {{menu.id}}.
        {{menu.menu_item}}.
        {{menu.price}}
      </p>
      </p>
    {{#if has_nuts}}
      <p class="has-nuts">
        Contains Nuts
      </p>
    {{/if}}
    </p>
    {{#if has_dairy}}
      <p class="has_dairy">
        Contains Dairy
      </p>
    {{/if}}
    </p>
    {{#if vegetarian}}
      <p class="vegetarian">
        Vegetarian Option
      </p>
    {{/if}}
    </p>
    {{#if gluten_free}}
      <p class="gluten_free">
        Gluten Free Option
      </p>
    {{/if}}
    </div>
  {{/each}}
</div>

